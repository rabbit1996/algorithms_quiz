    "def gradingStudents(grades):\n",
    "    result = []\n",
    "    for i in grades:\n",
    "        if i % 5 >= 3 and i >= 38:\n",
    "            result.append(i + i % 5)\n",
    "        else:\n",
    "            result.append(i)\n",
    "        \n",
    "    return result"
