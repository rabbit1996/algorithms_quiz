    "N = int(input())\n",
    "my_list = []\n",
    "for _ in range(N):\n",
    "    x,y = map(int,input().split())\n",
    "    my_list.append((x,y))\n",
    "\n",
    "my_list = sorted(my_list, key = lambda x:x[1])\n",
    "my_list = sorted(my_list, key = lambda x:x[0])\n",
    "\n",
    "for i in my_list:\n",
    "    print(\"{} {}\".format(i[0],i[1]))"
   ]
